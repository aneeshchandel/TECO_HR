}
}
return(flag)
}
########### Generate new parameter values
GenerateParamValues <- function(c_op){
flag = T
while (flag){
c_new = c_op + (runif((paramNum), - 0.5, 0.5))*(c_max - c_min)/10.0
if(isQualified(c_new)){
flag = F
}
}
return(c_new)
}
